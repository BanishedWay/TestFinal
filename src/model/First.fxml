               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
            <font>
                <Font name="Agency FB" size="18.0"/>
            </font>
            <padding>
                <Insets left="50.0"/>
            </padding>
        </Label>
    </AnchorPane>
    <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <ImageView fitHeight="270.0" fitWidth="897.0" pickOnBounds="true" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Image url="@/drawable/picture.jpg"/>
        </ImageView>
    </AnchorPane>
    <AnchorPane>
        <Label alignment="TOP_CENTER" contentDisplay="CENTER" lineSpacing="5.0" text="————————训练————————"
