            </center>
        </BorderPane>
        <BorderPane prefHeight="200.0" prefWidth="200.0">
            <center>
                <ImageView pickOnBounds="true" BorderPane.alignment="CENTER">
                    <Image url="@/drawable/picture5.jpg"/>
                </ImageView>
            </center>
        </BorderPane>
    </SplitPane>
    <SplitPane dividerPositions="0.5">
        <AnchorPane>
            <Button fx:id="btn3" mnemonicParsing="false" onAction="#event_start_predict" prefHeight="36.0"
