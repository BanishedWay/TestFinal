<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="900.0" prefWidth="600.0" stylesheets="@/style/First.css"
      xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fragment.FirstController">
    <AnchorPane>
        <effect>
            <Blend/>
        </effect>
        <Label alignment="TOP_CENTER" contentDisplay="CENTER" layoutX="267.0" prefHeight="24.0"
