            <center>
                <ImageView fitHeight="232.0" fitWidth="445.0" pickOnBounds="true" BorderPane.alignment="CENTER">
                    <Image url="@/drawable/picture3.jpg"/>
                </ImageView>
            </center>
        </BorderPane>
    </SplitPane>
    <SplitPane dividerPositions="0.5" prefHeight="38.0" prefWidth="897.0">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
            <Button fx:id="btn1" layoutX="114.0" layoutY="3.0" mnemonicParsing="false"
