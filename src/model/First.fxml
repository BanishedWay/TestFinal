            <center>
                <ImageView fitHeight="232.0" fitWidth="444.0" pickOnBounds="true" BorderPane.alignment="CENTER">
                    <Image url="@/drawable/picture2.jpg"/>
                </ImageView>
            </center>
        </BorderPane>
