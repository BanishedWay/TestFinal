        </AnchorPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
            <Button fx:id="btn2" layoutX="195.0" layoutY="7.0" mnemonicParsing="false"
                    onAction="#event_join_train" text="参与训练" AnchorPane.bottomAnchor="0.0"
                    AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
        </AnchorPane>
    </SplitPane>
    <AnchorPane>
        <Label alignment="TOP_CENTER" contentDisplay="CENTER" text="————————预测————————" wrapText="true"
               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
